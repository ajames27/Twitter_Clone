<!DOCTYPE html>
<html>
  <head>
    <title>Twit</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
    <%= yield %>
    <br />
    <%= link_to "Twitter Feed", feed_path, class:"btn btn-default" %>
    <br />
    <tr>
    <td><%= link_to 'Sign Out', destroy_user_session_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>

  </body>
</html>
